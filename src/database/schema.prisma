datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model DadJoke {
  id        Int      @id @default(autoincrement())
  setup     String
  punchline String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("dad_joke")
}
